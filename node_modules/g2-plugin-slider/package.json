{
  "name": "g2-plugin-slider",
  "version": "1.2.1",
  "description": "datazoom slider plugin for G2.",
  "keywords": [
    "g2",
    "slider",
    "plugin",
    "datazoom"
  ],
  "homepage": "https://github.com/antvis/g2-plugin-slider",
  "author": "sima.zhang1990@gmail.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:antvis/g2-plugin-slider.git"
  },
  "bugs": {
    "url": "https://github.com/antvis/g2-plugin-slider/issues"
  },
  "licenses": "MIT",
  "precommit": [
    "lint"
  ],
  "scripts": {
    "transpiler": "rm -rf lib && babel src --out-dir lib && lessc src/index.less lib/index.css",
    "build": "atool-build",
    "lint": "eslint --ext .js src",
    "test": "atool-test --no-chai",
    "doc": "atool-doc",
    "doc-build": "atool-doc --build",
    "test-keep": "atool-test -k --no-coverage",
    "ci": "tnpm run lint && tnpm run test",
    "gh-pages": "atool-doc --build && gh-pages -d __site"
  },
  "entry": {
    "slider": "./index"
  },
  "peerDependencies": {
    "g2": ">=2.3.0"
  },
  "devDependencies": {
    "atool-build": "0.6.x",
    "atool-doc": "^0.1.0",
    "atool-test": "^0.4.6",
    "babel-cli": "~6.6.4",
    "babel-core": "~6.7.4",
    "babel-plugin-add-module-exports": "~0.1.2",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-stage-0": "~6.5.0",
    "dora": "0.3.x",
    "expect.js": "~0.3.1",
    "jquery": "~2.1.4",
    "less": "~2.6.0",
    "pre-commit": "^1.1.2",
    "eslint": "~1.10.3",
    "eslint-config-airbnb": "~4.0.0",
    "g2": "^2.2.0",
    "gh-pages": "^0.11.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "pre-commit": [
    "lint"
  ]
}
